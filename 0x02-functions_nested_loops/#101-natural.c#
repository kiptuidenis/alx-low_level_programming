#include "main.h"
#include <stdio.h>
#include <stdlib.h>

/**
 * main - check the code.
 *
 * Return: Always 0.
 */
int main(void)
{
	int sum = 0;
	int base_num = 1023;

	while (base_num != 0)
	{
		mul_3 = base_num % 3;
		mul_5 = base_num % 5;

		if (mul_3 == 0)

			sum = sum + base_num;

		if (mul_5 == 0)
			sum = sum + base_num;

		base_num--;
	}

	return (0);
}