#include <stdlib.h>
#include <stdio.h>
#include <time.h>

/**
 *print_to_98 - prints numbers to 98
 *@n: The character to be checked
 */

void times_table(void);
{
	int tmp = 0;
	int i;
	int j;

	for (i = 0; i <= 9; i++)
	{
		int num = 0;
		int num2;

		printf("%d", num);
		putchar(',');
		putchar(' ');

		for (j = 0; j <= 8; j++)
		{
			num2 = num + tmp;

			printf("%d", num2);

			if (j < 8)
			{
				putchar(',');
				putchar(' ');
			}
		}

		tmp = tmp + 1;
	}
}
