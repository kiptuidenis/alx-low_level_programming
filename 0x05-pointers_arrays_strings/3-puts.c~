#include <stdio.h>
#include "main.h"

/**
 * _puts - prints string to stdout
 * @str: string to be printed
 */

void _puts(char *str)
{
	int str_val = 0;

	while (str[str_val] != '\0')
	{
		_putchar(str[str_val]);
		str_val = str_val + 1;
	}

	_putchar('\n');
}

/**
 * main - check the code
 *
 * Return: Always 0.
 */
int main(void)
{
    char *str;

    str = "I do not fear computers. I fear the lack of them - Isaac Asimov";
    _puts(str);
    return (0);
}
