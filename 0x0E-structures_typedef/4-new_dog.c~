#include "dog.h"
#include <stdio.h>

/**
 *new_dog - creates a new dog
 *@name: name of dog
 *@age: age of dog
 *@owner: Owner of dog
 *
 *Return: NULL if function fails. Otherwise new dog
 */

dog_t *new_dog(char *name, float age, char *owner)
{
	dog_t *new_dog;

	new_dog->name = name;
	new_dog->age = age;
	new_dog->owner = owner;

	if (new_dog == NULL)
		return (NULL);

	return (new_dog);
}
